\", marginTop: \"50px\", marginLeft: \"30%\"}} onSubmit={(e) => {\r\n      e.preventDefault();\r\n      CreateSession()\r\n    }}>\r\n      <Form.Group controlId=\"formBasicEmail\" >\r\n        <h3>Creer une session :</h3>\r\n        <Form.Control type=\"date\" placeholder=\"date\" id=\"date\"/>\r\n\r\n      </Form.Group>\r\n      <Form.Group controlId=\"formfirstname\">\r\n        <h4>Quelle sera la formation pour cette session ?</h4>\r\n      <Form.Control as=\"select\" id=\"formation\">\r\n        {formations.map((formation) => {\r\n            return  <option value={formation.id}>{formation.title}</option> \r\n        })}\r\n      </Form.Control>\r\n      \r\n      </Form.Group>\r\n      <Form.Group controlId=\"formfirstname\">\r\n        <h4>Dans quelle salle se déroulera cette session ?</h4>\r\n      <Form.Control as=\"select\" id=\"room\">\r\n        {rooms.map((room) => {\r\n            return  <option value={room.id}>{room.number}</option> \r\n        })}\r\n      </Form.Control>\r\n      \r\n      </Form.Group>\r\n\r\n      <Button variant=\"primary\" type=\"submit\">\r\n        Créer\r\n      </Button>\r\n    </Form>\r\n  )\r\n}\r\n\r\nexport default Session","/home/vivien/FormYou-React/src/Pages/Admin/index.jsx",["100"],"import {Card,Jumbotron} from 'react-bootstrap'\r\nimport {\r\n  BrowserRouter as Router,\r\n  Link\r\n} from \"react-router-dom\";\r\n\r\n\r\n\r\nconst Admin = () => {\r\n  return(\r\n    <div>\r\n    <Jumbotron>\r\n      <h1>Bienvenue sur votre page administrateur !</h1>\r\n      <p>\r\n        C'est à partir de cette page que vous pourrez créer une formation ou une session et gérer la liste des utilisateurs du site.\r\n      </p>\r\n      <p>\r\n      <Link to=\"/\" className=\"btn btn-primary btn mr-2\" >Revenir à l'accueil </Link>\r\n      </p>\r\n    </Jumbotron>\r\n    <Card style={{ width: '18rem' }}>\r\n      <Card.Body>\r\n      \r\n        <Card.Subtitle className=\"mb-2 text-muted\">Formations</Card.Subtitle>\r\n        <Card.