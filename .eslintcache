serId = useSelector(state => state.id)\r\n  console.log(userId)\r\n  const [email, setEmail] = useState('');\r\n  const [firstName, setFirstName] = useState('');\r\n  const [lastName, setLastName] = useState('');\r\n  const [sessions, setSessions] = useState([])\r\n\r\n  const loadInformations = (data) => {\r\n    setEmail(data.email);\r\n    setFirstName(data.firstname);\r\n    setLastName(data.lastname);\r\n  }\r\n\r\n  const getInformations = () => {\r\n    fetch(`https://api-rails-form-you.herokuapp.com/users/${userId}`, {\r\n      method: 'get',\r\n      headers: {\r\n        'Authorization': `Bearer ${Cookies.get('token')}`, \r\n        'Content-Type': 'application/json'\r\n      }\r\n    })\r\n    .then((response) => response.json())\r\n    .then((data) => loadInformations(data.data.attributes))\r\n  }\r\n\r\n  useEffect(() => {\r\n    getInformations();\r\n    loadSessions()\r\n  }, [])\r\n\r\n  const loadSessions = () => {\r\n    fetch(`https://api-rails-form-you.herokuapp.com/sessions?user_id=${userId}`, {\r\n      method: 'get',\r\n      headers: {\r\n        'Authorization': `Bearer ${Cookies.get('token')}`, \r\n        'Content-Type': 'application/json'\r\n      }\r\n      })\r\n      .then((response) => response.json())\r\n      .then((data) => setSessions(data))\r\n      .catch((error) => console.error(error))\r\n  }\r\n\r\n\r\n  return (\r\n    <>\r\n      <section>\r\n        <h1>Bienvenue sur votre profil {userId} !</h1>\r\n        <p>email : {email}</p>\r\n        <p>firstname : {firstName}</p>\r\n        <p>lastname : {lastName}</p>\r\n      </section>\r\n      <div id=\"sessions\">\r\n        {sessions.map((session) => {\r\n          return (\r\n            session.date\r\n          )})\r\n          }\r\n      </div>\r\n    </>\r\n  )\r\n};\r\n\r\nexport default Profile;","/home/vivien/FormYou-React/src/Pages/FormationDetail/index.jsx",[],"/home/vivien/FormYou-React/src/Pages/AdminFormation/index.jsx",["92","93","94","95"],"import {Form, Button}